      xsi:schemaLocation="http://www.springframework.org/schema/webflow
        				  http://www.springframework.org/schema/webflow/spring-webflow-2.0.xsd"
      start-state="inicio" >

    <var name="mbReporte" class="mx.ine.observadoresINE.mb.MBReportes" />
    <view-state id="inicio" view="index_reportes.xhtml">
        <on-entry>
            <evaluate expression="mbAdmin.dto.setIdModulo(null)" />
