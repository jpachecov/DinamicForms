public class FormCursos implements Serializable {

	/**
	 * Atributo necesario para la serealización de objetos
	 */
	private static final long serialVersionUID = -5554733153638297742L;
	
	private Integer idProceso;
	
	private Integer idDetalleProceso;
	
	private Integer origenCurso;
	
	private Date fecha;
	
	private Integer idAgrupacion;
	
	
	
	public FormCursos(){
