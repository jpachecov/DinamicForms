<!-- EN ESTE ARCHIVO SE DEFINEN TODAS LAS ACCIONES QUE SE PUEDEN EJECUTAR EN EL SISTEMA -->			  
			  
		<!-- EN LOS ARCHIVOS FLOW XML LAS TRANSICIONES (transitions) DEBEN DECLARARSE CON ESTOS VALORES
			 CUANDO SEAN OCUPADAS
		 -->	  
		<p:remoteCommand name="captura"  immediate="true"	action="captura"/>
		<p:remoteCommand name="consulta" immediate="true" 	action="consulta"/>
		<p:remoteCommand name="modifica" immediate="true" 	action="modifica"/>
		<p:remoteCommand name="elimina"  immediate="true"	action="elimina"/>
		<p:remoteCommand name="ejecuta"  immediate="true" 	action="ejecuta"/>
		
		
</ui:component>