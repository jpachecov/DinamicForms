package mx.ine.observadoresINE.enums;

 /**
 * Enum para determinar si un curso es impartido por el INE, OPLE o una agrupacion
 * @author Emmanuel García Ysamit
 * @since 03/07/2017
 * @copyright Direccion de sistemas - INE
 */
public enum EnumImparticionCursos {

	I("INE"),
	O("OPLE"),
	A("Agrupación");
	
	private final String description;
	
	 /**
     * Constructor para poder agregar el valor de la descripción de cada enum
     * @param description
     */
	EnumImparticionCursos(String description){
        this.description = description;
    }


    /**
     * Método que obtiene el valor de el atributo description
     *
     * @return String : valor que tiene el atributo description
     * 
     * @author Emmanuel García Ysamit
     * @since 03/07/2017
     */
    public String getDescription() {
        return description;
    }
}
