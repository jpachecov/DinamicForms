query_reporte_detalle_obs_OC												= SELECT OBS.NOMBRE || ' ' || OBS.APELLIDO_PATERNO || ' ' || OBS.APELLIDO_MATERNO AS field1, CLAVE_ELECTOR AS field2, EDAD as field3, SEXO AS field4, FECHA_SOLICITUDES as field5, CASE WHEN ORIGEN_SOLICITUD = 0 THEN 'INE' ELSE 'OPLE' END AS field6, CASE WHEN (NVL(OBS.ID_AGRUPACION, 0) > 0) THEN NOMBRE_AGRUPACION ELSE 'Individual' END AS field7, CASE WHEN EV.TIPO = 'A' THEN 'Acredito' ELSE CASE WHEN EV.TIPO = 'N' THEN 'No acredito' ELSE CASE WHEN EV.TIPO = 'P' THEN 'Pendiente' ELSE '' END END END AS field8, NVL(ID_CURSO,0) AS field9, FECHA_SESION AS field10, FOLIO AS field11 FROM OBSERVADORES OBS LEFT JOIN AGRUPACIONES AGR ON OBS.ID_AGRUPACION = AGR.ID_AGRUPACION AND OBS.ID_PROCESO_ELECTORAL = AGR.ID_PROCESO_ELECTORAL AND OBS.ID_DETALLE_PROCESO = AGR.ID_DETALLE_PROCESO INNER JOIN C_EVALUACION EV ON OBS.ID_EVALUACION = EV.ID_EVALUACION AND OBS.ID_PROCESO_ELECTORAL = EV.ID_PROCESO_ELECTORAL AND OBS.ID_DETALLE_PROCESO = EV.ID_DETALLE_PROCESO WHERE OBS.ID_PROCESO_ELECTORAL = :idProceso AND OBS.ID_DETALLE_PROCESO = :idDetalle

