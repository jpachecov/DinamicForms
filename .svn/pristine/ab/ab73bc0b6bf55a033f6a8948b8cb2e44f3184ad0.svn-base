package mx.ine.observadoresINE.dto;

import java.io.Serializable;

/**
 *
 * @author Pablo Zuñiga Mata
 * @since 05/04/2017
 * @copyright Direccion de sistemas - INE
 */
public class DTODetalleEstadoProceso implements Serializable {

    private static final long serialVersionUID = 3372786392242279220L;
    /**
     * Identificador de proceso
     */
    private Integer idProceso;
    /**
     * Identificador de Estado
     */
    private Integer idEstado;
    /**
     *  Nombre del estado
     */
    private String nombreEstado;

    /**
     * Método que obtiene el valor de el atributo idProceso
     *
     * @return Date : valor que tiene el atributo idProceso
     *
     * @author Pablo Zuñiga Mata
     * @since 05/04/2017
     */
    public Integer getIdProceso() {
        return idProceso;
    }

    /**
     * Método que ingresa el valor de el atributo idProceso
     *
     *
     * @author Pablo Zuñiga Mata
     * @param idProceso
     * @since 05/04/2017
     */
    public void setIdProceso(Integer idProceso) {
        this.idProceso = idProceso;
    }

    /**
     * Método que obtiene el valor de el atributo idEstado
     *
     * @return 
     *
     * @author Pablo Zuñiga Mata
     * @since 05/04/2017
     */
    public Integer getIdEstado() {
        return idEstado;
    }

    /**
     * Método que ingresa el valor de el atributo idEstado
     *
     *
     * @author Pablo Zuñiga Mata
     * @param idEstado
     * @since 05/04/2017
     */
    public void setIdEstado(Integer idEstado) {
        this.idEstado = idEstado;
    }

    /**
     * Método que obtiene el valor de el atributo nombreEstado
     *
     * @return Date : valor que tiene el atributo nombreEstado
     *
     * @author Pablo Zuñiga Mata
     * @since 05/04/2017
     */
    public String getNombreEstado() {
        return nombreEstado;
    }

    /**
     * Método que ingresa el valor de el atributo nombreEstado
     *
     *
     * @author Pablo Zuñiga Mata
     * @param nombreEstado
     * @since 05/04/2017
     */
    public void setNombreEstado(String nombreEstado) {
        this.nombreEstado = nombreEstado;
    }

}
